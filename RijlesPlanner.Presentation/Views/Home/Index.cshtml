@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div id="calendar"></div>
</div>

@section Scripts{
    <script>
          document.addEventListener('DOMContentLoaded', function() {
            var calendarEl = document.getElementById('calendar');
            var calendar = new FullCalendar.Calendar(calendarEl, {
                locale: 'nl',
                initialView: 'timeGridWeek',
                allDaySlot: false,
                selectable: true
            });
            calendar.render();
          });
    </script>
}